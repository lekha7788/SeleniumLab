package com.WebDriverDemos;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class D07LocateByClassName {

	public static void main(String[] args) {
		WebDriver driver = new ChromeDriver();
		driver.manage().window().maximize();
		driver.get("https://www.facebook.com/login/");
		driver.findElement(By.className("inputtext login_form_input_box")).sendKeys("lekha123@gmail.com");
		driver.findElement(By.className("inputtext login_form_input_box")).sendKeys("abc1234");
		

	}

}
